{
  "entryPoints": ["src/index.ts", "src/go-style.ts"],
  "out": "./docs/api",
  "exclude": ["**/*.test.ts", "**/test/**/*"],
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-vitepress-theme"
  ],
  "readme": "none",
  "docsRoot": "./docs",
  "name": "@tsports/go-colorful",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "skipErrorChecking": true,
  "tsconfig": "tsconfig.json",
  "categorizeByGroup": true,
  "defaultCategory": "API",
  "categoryOrder": [
    "Constructors",
    "Color Space Conversions",
    "Utilities",
    "Palettes",
    "Generators",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Document",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Method",
    "Parameter",
    "TypeParameter",
    "Accessor",
    "GetSignature",
    "SetSignature",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "Reference"
  ]
}
